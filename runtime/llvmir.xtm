(define *sys:precomp:current-output-port* #f)
(define *sys:precomp:current-lib-name* "xtmdylib")
(define *sys:precomp:func-defs-in-mod* '())

(define sys:precomp:currently-compiling?
  (lambda ()
    (or (output-port? *sys:precomp:current-output-port*)
        ;; this will be #t in a suppress-precomp-do form
        *sys:precomp:current-output-port*)))

(define impc:precomp:no-scm-suffix?
  (lambda (symname)
    (let ((tlist (llvm:get-function-args-withoutzone symname)))
      (member #f
              (map (lambda (type)
                     (let ((type-str (if (impc:ir:array? type)
                                         type
                                         (impc:ir:get-type-from-pretty-str type))))
                       (or (impc:ir:void? type-str)
                           (impc:ir:floating-point? type-str)
                           (impc:ir:fixed-point? type-str)
                           (impc:ir:pointer? type-str))))
                   (or tlist (list symname)))))))

(define impc:precomp:write-binding-details-to-file
  (lambda (lead-string symname type . args)
    (if (output-port? *sys:precomp:current-output-port*)
        (let* ((type-str (if (or (string=? (atom->string type) "polying")
                                 (string=? (atom->string lead-string) "Lib Load:"))
                             "null"
                             (impc:ir:pretty-print-type (atom->string type))))
               (precomp-string
                (cond ((string=? lead-string "Overload:")
                       (string-append "" "(bind-poly " symname ")\n"))
                      ((string=? lead-string "SetValue:")
                       (string-append
                        (if (impc:ir:array? (impc:ir:get-type-from-pretty-str type-str))
                            "(bind-val "
                            (string-append "(bind-lib-val "
                                           *sys:precomp:current-lib-name*
                                           " ")) ;; bind-lib-val only works for floats and ints
                        symname " " type-str ")\n"))
                      ((string=? lead-string "LibBound:")
                       (string-append "(bind-lib "
                                      symname  " "
                                      type-str ")\n"))
                      ((string=? lead-string "Compiled:")
                       (if (assoc-strcmp symname *sys:precomp:func-defs-in-mod*)
                           ""
                           (begin
                             (set! *sys:precomp:func-defs-in-mod* (cons (cons symname #t) *sys:precomp:func-defs-in-mod*))
                             (string-append "(bind-lib-func"
                                            (if (impc:precomp:no-scm-suffix? symname) "-no-scm" "")
                                            " " *sys:precomp:current-lib-name* " "
                                            symname " " type-str
                                            (if (null? args) ""
                                                (string-append " " (number->string (car args))))
                                            ")\n"))))
                      ((string=? lead-string "DataType:")
                       (string-append "(bind-lib-type "
                                      *sys:precomp:current-lib-name* " " symname " " type-str ")\n"))
                      ((string=? lead-string "SetAlias:")
                       (string-append "(bind-alias " symname " " type-str ")\n"))
                      ((string=? lead-string "Lib Load:")
                       (let ((dylib-sym-name (if *sys:precomp:current-load-dylib-info*
                                                 (atom->string (car *sys:precomp:current-load-dylib-info*))
                                                 "unknown-libname"))
                             (dylib-path (if *sys:precomp:current-load-dylib-info*
                                             (cdr *sys:precomp:current-load-dylib-info*)
                                             "unknown-dylib-path")))
                         (string-append "(define " dylib-sym-name " (sys:load-dylib \"" dylib-path "\"))\n"
                                        "(if (not " dylib-sym-name ")\n    (begin (print-with-colors *impc:compiler:pretty-print-error-color* 'default #t (print \"\\nError\"))(print \": could not load " symname " dynamic library\")(error \"\")))\n")))
                      (else ""))))
          (display precomp-string *sys:precomp:current-output-port*)))))

(define impc:compiler:print-binding-details-to-log
  (lambda (lead-string symname type)
    (impc:precomp:write-binding-details-to-file lead-string
                                                (atom->string symname)
                                                type)
    (if (equal? *impc:compiler:message:level* 'high)
        (begin
          (print lead-string " ")
          (print-with-colors *impc:compiler:pretty-print-name-color* 'default #t (print symname))
          (print " >>> ")
          (print-with-colors *impc:compiler:pretty-print-type-color* 'default #f  (print type))
