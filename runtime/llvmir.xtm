(define impc:ir:gpoly-types
  (lambda (name arity type)
    ;; (println 'poly: name 'a: arity 't: type)    
    (if (symbol? name) (set! name (symbol->string name)))
    ;; (println 'type_a: type)
    (cond ((and type (list? type))
           (set! type (map (lambda (x)
                             (if (string? x)
                                 (if (regex:match? x "_poly_")
                                    (cadr (regex:matched x "%(.*)_poly_.*"))
                                    (cadr (regex:matched x "%([^-*]*)")))
                                 (if (and (symbol? x)
                                          (regex:match? (symbol->string x) ":"))
                                     (car (regex:split (symbol->string x) ":"))
                                     (if (impc:ir:type? x)
                                         (impc:ir:pretty-print-type x)
                                         "_"))))
                           (if (and (number? (car type))
                                    (impc:ir:closure? type))
                               (cddr type)
                               type)))
           ;; if lgth(type) = arity then we only have args
           ;; and we should add a "_" return type
           (if (= (length type) arity)
               (set! type (cons "_" type))))
          ((and type (string? type))
           (let ((ags (impc:ir:get-pretty-closure-arg-strings type)))
             (set! type (map (lambda (x)
                               (if (regex:match? x ":")
                                   (car (regex:type-split x ":"))
				   (if (regex:match? x "^\\!")
				       "_"
				       x)))
                             ags))))
          (else (set! type (make-list (+ 1 arity) "_"))))
    ;; (println 'type_b: type)
    (let* ((tmp (assoc-strcmp-all (string->symbol name) *impc:ir:gpolys*))           
           (res (cl:remove-if (lambda (x) (<> arity (cadr x))) tmp)))
      ;; (println 'res: (map (lambda (gp) (car (cddddr gp))) res))
      (if (null? res)
          #f
          (let* ((weights (map (lambda (gp)
                                 (cons (apply + (map (lambda (x y)
                                                       ;; (println x '=? y)
                                                       (if (string=? x "_")
                                                           0
                                                           (if (string=? x y)
                                                               1
                                                               (if (string=? y "_")
                                                                   0
                                                                   -1))))
                                                       ;; (if (string=? x y)
                                                       ;;     1
                                                       ;;     (if (string=? x "_")
                                                       ;;         0
                                                       ;;         -100))) ;; you lose!
                                                     (car (cddddr gp))
                                                     type))
                                       gp))
                               res))
                 (w (apply max (map (lambda (x) (car x)) weights))))
            ;; (println '++++++++++++++ (length weights))
            ;; (println (for-each (lambda (k)
            ;;                      (println k))
            ;;                    weights))
            ;; (println 'best: w ': (length weights) '-> (assoc w weights))
            (if (> (length (assoc-strcmp-all w weights)) 1)
		(print-error 'Compiler 'Error: 'ambiguous 'generic 'overload name '-> type))	    
	    (cdddr (assoc w weights)))))))

