					(if (impc:ir:array? ttype)
					    (caddr ttype)
					    (impc:ir:pointer-- (cadr var)))))
             (val (impc:ir:gname)))
         ;; type tests
         (if (not (impc:ir:pointer? ttype))
             (print-error 'Compiler 'Error: 'Type 'Mismatch: ast 'array 'must 'be 'pointer 'not (cadr var)))
