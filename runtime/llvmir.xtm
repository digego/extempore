      ;; arg 1 must be a closure
      ;; arg 2 must be a string
      ;; arg 3 must be a string
      ;; arg 4 must be of type!      
      (let* ((os (make-string 0))
             (closure-str (if (and (symbol? (cadr ast))
                                   (llvm:get-function (symbol->string (cadr ast))))
