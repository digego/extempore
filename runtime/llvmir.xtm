             (print-error 'Compiler 'Error: 'Type 'Mismatch: ast 'index 'must 'be 'fixed-point 'not (cadr idx)))
	 (if (and (= 1.0 (impc:ir:get-ptr-depth ttype))
		  (or (impc:ir:closure? ttype)
		      (impc:ir:tuple? ttype)))
