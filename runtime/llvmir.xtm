      (let* ((os (make-string 0))
             (loop-num (llvm:count++))
             (loop-label (string-append "loop" (number->string loop-num) ":"))
             (closeloop-label (string-append "closeloop" (number->string loop-num) ":"))
             (after-label (string-append "after" (number->string loop-num) ":"))
