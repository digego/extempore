(define impc:ir:gpoly-src-changed
  (lambda (name arity)
    (if (string? name) (set! name (string->symbol name)))
    (let ((res (member (cons name arity) *impc:ir:gpoly-needs-update*)))
      ;; (println 'name: name 'res: res)
