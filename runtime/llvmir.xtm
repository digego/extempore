(define impc:ir:check-poly
  (lambda (name ftype)
    (if (string? ftype)
	(set! ftype (impc:ir:get-type-from-pretty-str ftype)))
    (let ((res (assoc-strcmp name *impc:ir:polys*)))
      (if res
	  (let* ((r2 (map (lambda (p)
			    ;(println 'ftype: ftype 'p: p)
			    ;;(if (impc:ir:type-match? (cadr p) ftype)
			    (if (equal? (cadr p) ftype)
				(car p)
				'()))
			  (cdr res)))
		 (r3 (remove '() r2)))
	    (if (null? r3) #f (car r3)))
	  #f))))



