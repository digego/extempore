    ;;(print 't: t)
    (if (null? t) '()
        (if (atom? t)
            (impc:ir:get-type-from-pretty-str (sexpr->string t))
            (map (lambda (t)
                   (impc:ir:convert-from-pretty-types t))
                 t)))))


