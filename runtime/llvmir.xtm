	(if (not (impc:ir:fixed-point? t))
	    (impc:compiler:print-bad-type-error-with-ast (impc:ir:get-type-str t) "alloc size must be an integer" ast))
	(emit idx-str os)
	;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;
	(if (not (member t (list *impc:ir:si64* *impc:ir:ui64*)))
