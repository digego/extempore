    (let* ((os (make-string 0))
           (num (number->string (llvm:count++)))
           ;; (name-str (impc:ir:compiler (caddr ast) types))
           ;; (name (impc:ir:gname))
           (closure-str (if (and (symbol? (cadr ast))
