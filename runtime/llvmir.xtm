(define impc:ir:generics-apply-macros
  (lambda (ast)
    (cond ((atom? ast) ast)
          ((and (list? ast)
                (symbol? (car ast))
                (not (eq? 'dotimes (car ast)))
                (defined? (car ast))                
                (macro? (eval (car ast))))
           (macro-expand ast))
          ((pair? ast)
           (cons (impc:ir:generics-apply-macros (car ast))
                 (impc:ir:generics-apply-macros (cdr ast))))
          (else ast))))

(define impc:ir:add-gpoly
  (let ((pcnt 100))
    (lambda (code)
      ;; (println 'adding: code)
      (set! pcnt (+ pcnt 1))      
      ;; (println 'code-pre-macro: code)
      ;; apply any macros to generic code!
      (set! code (cons (car code)
                       (list (cadr code)
                             (impc:ir:generics-apply-macros (caddr code)))))
      ;; (println 'code-post-macro: code)
      (if (not (regex:match? (symbol->string (cadr code)) ":"))
	  (impc:compiler:print-compiler-error "generic functions must supply type"))
      (let* ((res (regex:type-split (symbol->string (cadr code)) ":"))
             (name (string->symbol (car res)))
             ;; (ftype (string->symbol (cadr res))))
             (type (cadr res))
             (syms (cl:remove-duplicates (regex:match-all type "![a-zA-Z0-9_]*")))
             (newsyms (map (lambda (s)
                             (string-append "!g" 
                                            (substring s 1 (string-length s))
                                            "_"
                                            (number->string pcnt)))
                           syms))
             (newtype (string->symbol (regex:replace-everything type syms newsyms)))
             (newtypematch (map (lambda (k) (if (regex:match? k ":")
						(car (regex:type-split k ":"))
						(if (regex:match? k "^\\!g")
						    "_"
						    k)))
                                (impc:ir:get-pretty-closure-arg-strings (symbol->string newtype))))
             (arity (- (length (impc:ir:get-pretty-closure-arg-strings (symbol->string newtype))) 1))
             (newcode (list 'bind-func
                            (string->symbol (string-append (symbol->string name)
                                                           ":"
                                                           (symbol->string newtype)))
                            (caddr code))))
        (let ((v (cl:remove-if (lambda (x)
                                 (or
                                  (<> arity (cadr x))
                                  (not (string=? (symbol->string name) (symbol->string (car x))))
                                  (member #f
				  	  (map (lambda (xx yy)
						 ;; (println 'xx: xx 'yy: yy)
				  		 (let ((res (if (regex:match? xx "^\\!g")
								(string=?
								 (car (regex:type-split yy "_"))
								 (car (regex:type-split xx "_")))
								(string=?
								 (car (regex:type-split yy ":"))
								 (car (regex:type-split xx ":"))))))
						   ;; (println 'res: res)
				  		   res))
				  	       (impc:ir:get-pretty-closure-arg-strings (symbol->string newtype))
				  	       (impc:ir:get-pretty-closure-arg-strings (symbol->string (caddr x)))))))
                               *impc:ir:gpolys*)))
	  ;; (if (not (null? v))
	  ;;     (println 'updating: name 'with newtype)
	  ;;     (println 'adding: name 'with newtype)) 
          (if (not (null? v))
              (set-cdr! (car v) (list arity newtype newcode newtypematch))
              (set! *impc:ir:gpolys* (cons (list name arity newtype newcode newtypematch) *impc:ir:gpolys*)))
          (set! *impc:ir:gpoly-needs-update* (cons (cons name arity) *impc:ir:gpoly-needs-update*))
          #t)))))


