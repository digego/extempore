(xtmtest (bind-func test_mutex
  (lambda ()
    (let ((mutex (mutex_create)))
      (mutex_lock mutex)
      (mutex_trylock mutex) ; this is allowed to spuriously fail, would that affect this test?
      (mutex_unlock mutex)
      (mutex_unlock mutex)
      (mutex_destroy mutex))))
  (test_mutex))

